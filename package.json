{
  "name": "tepper",
  "version": "1.1.0",
  "description": "Modern library for testing HTTP servers",
  "main": "dist/tepper.js",
  "engines": {
    "node": ">= 14"
  },
  "homepage": "https://github.com/DanielRamosAcosta/tepper",
  "repository": "https://github.com/DanielRamosAcosta/tepper",
  "author": "Daniel Ramos <danielramosacosta@hotmail.com>",
  "contributors": [
    "Daniel Ramos <danielramosacosta@hotmail.com>",
    "Aitor Alonso <aitor@aalonso.dev>"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --ext .ts --fix ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.5",
    "@types/express": "4.17.20",
    "@types/mime-types": "2.1.3",
    "@types/multer": "1.4.8",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-c8": "0.31.4",
    "cookie-parser": "1.4.6",
    "eslint": "8.51.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "express": "4.18.2",
    "multer": "1.4.4",
    "prettier": "2.8.8",
    "rimraf": "4.4.1",
    "typescript": "5.2.2",
    "vitest": "0.31.4"
  },
  "dependencies": {
    "@types/node-fetch": "2.6.6",
    "form-data-encoder": "^1.8.0",
    "formdata-node": "^4.3.2",
    "mime-types": "^2.1.34",
    "node-fetch": "^2.6.1"
  }
}