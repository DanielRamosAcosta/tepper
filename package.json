{
  "name": "tepper",
  "version": "1.0.0",
  "description": "Modern library for testing HTTP servers",
  "main": "dist/tepper.js",
  "engines": {
    "node": ">= 12.13.0"
  },
  "homepage": "https://github.com/DanielRamosAcosta/tepper",
  "repository": "https://github.com/DanielRamosAcosta/tepper",
  "author": "Daniel Ramos <danielramosacosta@hotmail.com>",
  "contributors": [
    "Daniel Ramos <danielramosacosta@hotmail.com>",
    "Aitor Alonso <aitor@aalonso.dev>"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --ext .ts --fix ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/mime-types": "2.1.1",
    "@types/multer": "1.4.7",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "@vitest/coverage-c8": "^0.23.1",
    "cookie-parser": "1.4.6",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "express": "4.18.1",
    "multer": "^1.4.5-lts.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.2",
    "vitest": "^0.23.1"
  },
  "dependencies": {
    "@types/node-fetch": "2.6.2",
    "form-data-encoder": "^1.7.1",
    "formdata-node": "^4.3.2",
    "mime-types": "^2.1.34",
    "node-fetch": "^2.6.1"
  }
